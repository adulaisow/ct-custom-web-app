{
  "routes": [
    {
      "route": "**",
      "rewrite": "/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": ["/*.{png,jpg,gif}", "/*.{js.map,js,txt}"]
  },
  "responseOverrides": {
    "403": {
      "rewrite": "/index.html",
      "statusCode": 200
    },
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}